    "gatsby-plugin-netlify-identity": "0.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "faunadb": "^3.0.1",
    "generate-password": "^1.5.1",
    "netlify-lambda": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
